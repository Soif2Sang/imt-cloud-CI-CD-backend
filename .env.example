# CI/CD Backend Environment Variables
# Copy this file to .env and update the values

# Database connection string (PostgreSQL running in Docker)
DATABASE_URL=postgres://cicd:cicd_password@localhost:5432/cicd_db?sslmode=disable

# API Server configuration
API_PORT=8080

# PostgreSQL Configuration (used by docker-compose)
POSTGRES_USER=cicd
POSTGRES_PASSWORD=cicd_password
POSTGRES_DB=cicd_db

export JWT_SECRET=un_super_secret
export GOOGLE_CLIENT_ID=
export GOOGLE_CLIENT_SECRET=
export API_URL=http://localhost:8080
export FRONTEND_URL=http://localhost:5173
